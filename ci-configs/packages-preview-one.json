{
  "target_repo": {
    "url": "https://github.com/MicrosoftDocs/azure-docs-sdk-python",
    "branch": "smoke-test",
    "folder": "preview"
  },
  "packages": [
    {
      "package_info": {
        "install_type": "pypi",
        "prefer_source_distribution": "true",
        "name": "azure-iot-hub",
        "version": "2.2.1rc2"
      },
      "exclude_path": [
        "test*",
        "example*",
        "sample*",
        "doc*"
      ]
    }
  ],
  "required_packages": [
    {
      "install_type": "pypi",
      "name": "azure-mgmt-core"
    },
    {
      "install_type": "pypi",
      "name": "azure-storage-blob",
      "version": "12.6.0b1"
    },
    {
      "install_type": "pypi",
      "name": "azure-core"
    },
    {
      "install_type": "pypi",
      "name": "aiohttp"
    }
  ],
  "run_post_toc_script": false
}
