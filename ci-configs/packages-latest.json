{
  "target_repo": {
    "branch": "smoke-test",
    "url": "https://github.com/MicrosoftDocs/azure-docs-sdk-python"
  },
  "packages": [
    {
      "package_info": {
        "name": "azure-cognitiveservices-speech",
        "install_type": "pypi",
        "prefer_source_distribution": false,
        "version": "==1.46.0"
      },
      "exclude_path": [
        "test*",
        "example*",
        "sample*",
        "doc*"
      ],
      "extension_config": {
        "autodoc_default_options": {
          "inherited-members": 1
        }
      }
    }
  ],
  "required_packages": [
    {
      "install_type": "pypi",
      "name": "vcrpy"
    },
    {
      "install_type": "pypi",
      "name": "python-dateutil"
    },
    {
      "version": "2018.4.16",
      "name": "certifi",
      "install_type": "pypi"
    },
    {
      "install_type": "pypi",
      "name": "isodate"
    },
    {
      "install_type": "pypi",
      "name": "requests_oauthlib"
    },
    {
      "version": "0.5.0",
      "name": "adal",
      "install_type": "pypi"
    },
    {
      "version": "12.0.2",
      "name": "keyring",
      "install_type": "pypi"
    },
    {
      "install_type": "pypi",
      "name": "grpcio"
    },
    {
      "install_type": "pypi",
      "name": "aiohttp"
    },
    {
      "install_type": "pypi",
      "name": "pandas"
    }
  ],
  "run_post_toc_script": false
}
